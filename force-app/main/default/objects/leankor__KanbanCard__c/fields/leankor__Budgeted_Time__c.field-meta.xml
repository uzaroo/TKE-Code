<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>leankor__Budgeted_Time__c</fullName>
    <deprecated>false</deprecated>
    <description>This field shows total time given to complete the task/tasks on &apos;KanbanCard&apos;.</description>
    <externalId>false</externalId>
    <formula>if(TEXT(leankor__DurationUnits__c)==&quot;Minutes&quot;,(leankor__EstimatedDuration__c/60),leankor__EstimatedDuration__c*if(TEXT(leankor__DurationUnits__c)==&quot;Hours&quot;,1,if(TEXT(leankor__DurationUnits__c)==&quot;Days&quot;,8,if(TEXT(leankor__DurationUnits__c)==&quot;Weeks&quot;,5*8,if(TEXT(leankor__DurationUnits__c)==&quot;Months&quot;,20*8,12*20*8)))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>This field shows total time given to complete the task/tasks on &apos;KanbanCard&apos;.</inlineHelpText>
    <label>Budgeted Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
