<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>leankor__EffortRemainingTime__c</fullName>
    <deprecated>false</deprecated>
    <description>This fields converts Effort Remaining Value in to Time ( 1 Day = 8 Hrs.)</description>
    <externalId>false</externalId>
    <formula>if(TEXT(leankor__DurationUnits__c)==&quot;Minutes&quot;,(leankor__EffortRemaining__c/60),leankor__EffortRemaining__c*if(TEXT(leankor__DurationUnits__c)==&quot;Hours&quot;,1, leankor__WorkingDays__c*8))</formula>
    <inlineHelpText>This fields converts Effort Remaining Value in to Time ( 1 Day = 8 Hrs.)</inlineHelpText>
