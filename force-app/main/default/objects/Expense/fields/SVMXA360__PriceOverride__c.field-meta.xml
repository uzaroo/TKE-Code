<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXA360__PriceOverride__c</fullName>
    <deprecated>false</deprecated>
    <description>This field when true indicates that the Total Entitled Price and Total Price don&apos;t match.</description>
    <externalId>false</externalId>
    <formula>SVMXA360__TotalEntitledPrice__c  &lt;&gt;   ( UnitPrice *  Quantity  - ( Discount / 100 * UnitPrice ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Price Override</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
