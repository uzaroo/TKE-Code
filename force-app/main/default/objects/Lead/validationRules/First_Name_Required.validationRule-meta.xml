<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_Name_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK(FirstName) , ISNEW(), NOT( $Permission.TKE_Bypass_Validation ))</errorConditionFormula>
    <errorDisplayField>FirstName</errorDisplayField>
    <errorMessage>Complete this field</errorMessage>
</ValidationRule>
