<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSL__Deny_Objectives_Negative_Weight</fullName>
    <active>true</active>
    <errorConditionFormula>OR(FSL__Service_Clustering_Weight__c &lt; 0,FSL__Service_Priority_Weight__c &lt; 0)</errorConditionFormula>
    <errorMessage>Policy objectives weight can not be negative</errorMessage>
</ValidationRule>
