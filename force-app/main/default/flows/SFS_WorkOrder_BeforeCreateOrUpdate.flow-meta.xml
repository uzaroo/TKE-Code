    <decisions>
        <name>Check_Resulting_Value</name>
        <label>Check Resulting Value</label>
        <locationX>182</locationX>
        <locationY>803</locationY>
        <defaultConnector>
            <targetReference>Check_WO_Location</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Set_Duration_WO</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varResultingQuotient</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>varResultInNumber</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Duration</targetReference>
            </connector>
            <label>Set Duration</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_WO_Location</name>
        <label>WO Location Null</label>
        <locationX>182</locationX>
        <locationY>1103</locationY>
        <defaultConnector>
            <targetReference>Set_Service_Territory</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.LocationId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>WO_Asset_Null</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>IsCaseSetAndSubTypeIsEntrapment</name>
        <label>IsCaseSetAndSubTypeIsEntrapment</label>
        <locationX>182</locationX>
        <locationY>395</locationY>
        <defaultConnector>
            <targetReference>Get_Multi_Day_Duration</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Sub_Type_Entrapment</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.CaseId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Case.Case_Sub_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Entrapment</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Derive_fields_from_Case</targetReference>
            </connector>
            <label>Sub Type = Entrapment</label>
        </rules>
    </decisions>
    <decisions>
        <description>Decision to check if the record is created or updated.</description>
        <name>Record_Action</name>
        <label>Record Action</label>
        <locationX>479</locationX>
        <locationY>287</locationY>
        <defaultConnectorLabel>Update</defaultConnectorLabel>
        <rules>
            <name>Create</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>IsNew</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>IsCaseSetAndSubTypeIsEntrapment</targetReference>
            </connector>
            <label>Create</label>
        </rules>
    </decisions>
