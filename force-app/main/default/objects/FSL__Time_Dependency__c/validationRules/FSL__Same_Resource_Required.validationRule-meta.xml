<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FSL__Same_Resource_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(FSL__Dependency__c , &apos;Immediately Follow&apos;), FSL__Same_Resource__c = FALSE)</errorConditionFormula>
    <errorDisplayField>FSL__Dependency__c</errorDisplayField>
    <errorMessage>Immediately Follow dependency requires Same Resource</errorMessage>
</ValidationRule>
